{
  "url": "http://webdev-wonder.herokuapp.com",
  "fileStorage": false,
  "server": {
    "port": process.env.PORT,
    "host": "0.0.0.0"
  },
  "database": {
    "client": "postgres",
    "connection": {
      "host": process.env.POSTGRES_HOST,
      "user": process.env.POSTGRES_USER,
      "password": process.env.POSTGRES_PASSWORD,
      "database": process.env.POSTGRES_DATABASE,
      "port": "5432"
    },
    "debug": false,
    "mail": {
      "transport": "Direct"
    },
    "logging": {
      "transports": ["file", "stdout"]
    },
    "process": "local",
    "paths": {
      "contentPath": "/Users/i335240/sap/creative/ghost-blog/content"
    }
  },
  "storage": {
    "active": "ghost-s3",
    "ghost-s3": {
      "accessKeyId": "aws access key from IAM service",
      "secretAccessKey": "aws secret key from IAM service",
      "bucket": "bucket-name",
      "region": "aws region where S3 bucket is located",
      "assetHost": "s3 bucket url"
    }
  }
}
